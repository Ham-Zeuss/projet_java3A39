<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="Controller.Maryem.FrontDisplayProfilesController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <!-- Container for profile cards -->
    <FlowPane fx:id="profilesContainer" hgap="15.0" vgap="15.0" alignment="CENTER" prefWrapLength="1445.0">
        <!-- Profile cards added dynamically -->
    </FlowPane>

    <Label fx:id="errorLabel" text="" style="-fx-text-fill: red;"/>
</VBox>